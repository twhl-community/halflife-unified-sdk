  startdark(choices) : "Level Fade In" : 0 =
  [ 
    0 : "No"
    1 : "Yes"
  ]
  gametitle(choices) : "Display game title" : 0 = 
  [ 
    0 : "No"
    1 : "Yes"
  ]
